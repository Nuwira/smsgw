{
    "name": "nuwira/smsgw",
    "description": "Nuwira SMS bridge client library",
    "type": "library",
    "authors": [
        {
            "name": "Amalina Kurniasari",
            "email": "nia@nuwira.co.id"
        },
        {
            "name": "Muhammad Zamroni",
            "email": "zam@nuwira.co.id"
        }
    ],
    "require": {
        "illuminate/support": "~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "propaganistas/laravel-phone": "~2.0|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Nuwira\\Smsgw\\": "src/"
        }
    },
    "config":{  
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Nuwira\\Smsgw\\SmsServiceProvider"
            ],
            "aliases": {
                "SMS": "Nuwira\\Smsgw\\SmsFacade"
            }
        }
    }
}
